<!DOCTYPE html>
<html>
  <head>
    <%= favicon_link_tag 'favicon.ico' %>
    <title><%= "My Messaging Store" || yield(:title) %></title>
    <meta name="description"
        content="<%= ("Commerce for the messaging age" || "").squish %>">
    <%= csrf_meta_tags %>

    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <%= stylesheet_link_tag    'application', media: 'all' %>
  </head>
  <body>
    <%#= render 'shared/navbar' %>
    <%#= render 'shared/navbarhidder' %>
    <%= render 'shared/flashes' %>
    <!--     <p class="notice"><%= notice %></p> -->
    <!--     <p class="alert"><%= alert %></p>  -->
    <%= yield %>
    <% unless @no_footer %>
      <%#= render "shared/footer" unless @skip_footer %>
    <% end %>

    <!--     Google Analytics script -->
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-40122648-3', 'auto');
      ga('send', 'pageview');


    </script>

    <%= javascript_include_tag 'application' %>



<!--     "Message Us" Plugin from Facebook  -->
    <script>


    </script>

  </body>
</html>
